<main class="contenedor contenedor-formularios">
    <h1><%= nombrePagina%></h1>

    <form action="/inciar-sesion" method="POST" class="default-form">
        <div class="campo">
            <label for="email">E-Mail</label>
            <input type="email" name="email" placeholder="Tu Email">
        </div>

        <div class="campo">
            <label for="password">Password</label>
            <input type="password" name="password" placeholder="Tu Password">
        </div>

        <div class="campo enviar">
            <input type="submit" value="Inciar Sesion" class="btn btn-rosa">
        </div>


    </form>
</main>